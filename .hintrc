{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "grid-column",
          "grid-row",
          "flex-direction",
          "order",
          "width: fit-content",
          "scroll-behavior",
          "-webkit-text-fill-color",
          "resize"
        ]
      }
    ],
    "compat-api/html": "off"
  }
}